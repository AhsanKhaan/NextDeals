{
  "name": "Next Affiliate Project",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    
    "mongoose": "8.16.5",
    "mongoose-paginate-v2": "1.9.1",
    "mongodb": "6.3.0",
    
    
    "payload": "3.49.0",
    "@payloadcms/db-mongodb": "3.49.0",
    "@payloadcms/next": "3.49.0",
    "@payloadcms/payload-cloud": "3.49.0",
    "@payloadcms/richtext-lexical": "3.49.0",
    "@payloadcms/ui": "3.49.0",
    
    
    "react": "19.1.0",
    "react-dom": "19.1.0",
    
    
    "next": "15.3.2",
    
    
    "framer-motion": "10.16.16",
    "lottie-react": "2.4.0",
    "lucide-react": "0.294.0",
    "tailwind-merge": "2.0.0",
    "tailwindcss-animate": "1.0.7",
    "sonner": "1.7.1",
    "next-themes": "0.2.1",
    
    
    "react-hook-form": "7.54.1",
    "@hookform/resolvers": "3.9.1",
    "zod": "3.24.1",
    "vaul": "latest",

    "cross-env": "7.0.3",
    "dotenv": "16.4.7",
    "sharp": "0.34.2",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "input-otp": "1.4.1",
    "react-countup": "6.5.0",
    "react-day-picker": "9.8.0",
    "react-intersection-observer": "9.5.3",
    "react-parallax-tilt": "1.7.156",
    "react-resizable-panels": "2.1.7",
    "recharts": "2.15.0",
    "@radix-ui/react-slot": "^1.0.2",

  },
  "devDependencies": {

    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "jsdom": "26.1.0",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "vitest": "3.2.3",
    

    "@types/node": "22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "typescript": "5.7.3",
    

    "@vitejs/plugin-react": "4.5.2",
    "vite-tsconfig-paths": "5.1.4",
    

    "eslint": "9.16.0",
    "eslint-config-next": "15.3.0",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.4.0",
    

    "postcss": "8.5.0",
    "autoprefixer": "10.4.20",
    "tailwindcss": "3.4.17"
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}